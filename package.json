{
  "name": "slack-channel-grouping",
  "version": "0.0.0",
  "description": "Grouping Slack channels by prefix",
  "main": "",
  "scripts": {
    "dev": "webextension-toolbox dev",
    "build": "webextension-toolbox build",
    "lint": "eslint ./app/scripts/**/*",
    "lint:fix": "eslint ./app/scripts/**/* --fix",
    "supported-browsers": "yarn browserslist",
    "bundle-source": "zip -r storage/source.zip . -x '.git/*' 'node_modules/*' 'dist/*' 'packages/*' 'storage/*' '.idea/*'"
